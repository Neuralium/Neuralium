{
  "AppSettings": {
    "SystemFilesPath": "/home/jdb/demo/peers/peer1",
    //  "SystemFilesPath" : "/home/jdb/demo/tests/super/files/peers/peer1",
    "logLevel": "3",
    "EnableHubs": true,
    "nodes": [
  //  {"ip" : "127.0.0.1", "port" : 33889}
   //   {"ip" : "127.0.0.1", "port" : 33888}
     // {"ip": "34.66.138.235", "port": 33887}

//      {"ip" : "35.225.199.252", "port" : 33888}
    ],
    "RpcLoggingLevel" : "Verbose",
    "SocketType" : "Stream",
    "HubsWebAddress" : "http://localhost:5000",
    "NeuraliumChainConfiguration": {

      //  "PassphraseCaptureMethod" : "Console",
      "LoadWalletOnStart": false,
      "CreateMissingWallet": false,
      "BlockSavingMode": "DigestThenBlocks",
      "DisableSync": false,
      "MinimumSyncPeerCount": 1,
      "MinimumDispatchPeerCount" : 1,
      "SkipGenesisHashVerification": true,
      "SkipDigestHashVerification" : true,
      "ElectionsRegistrationMethod": "Web",
      "RegistrationMethod": "Web",
      "EnableMiningStatusChecks" : true,
      "TrackedSnapshotAccountsList" : ["{SF}"],
      "AccountSnapshotTrackingMethod" : "List",
      "CompressWallet" : true,
      "EncryptWallet" : false,

     // "WebElectionsRegistrationUrl": "http://localhost:5000",
     //   "WebRegistrationUrl" : "http://localhost:5000",
      // "UseKeyLog" : false

      // ,  "TransactionXmssKeyTreeHeight" : 2,
      "TransactionXmssKeyTreeHeight" : 5,
      "MessageXmssKeyTreeHeight" : 5,
      "ChangeXmssKeyTreeHeight" : 5
     ,// "PassphraseCaptureMethod" : "Console",
    },
    "UndocumentedDebugConfigurations": {
      "DebugNetworkMode": false,
      "localhostOnly": false,
      "SkipHubCheck": true
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Information"
      }
    }
  },
  "Serilog": {
    "MinimumLevel":{
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    //https://github.com/serilog/serilog-sinks-console
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties}{NewLine}{Exception}"
              }
            }
          ]
        }
      }],
    "Enrich": ["FromLogContext", "WithThreadId" , "WithExceptionDetails"],
    "Properties": {

    }
  }
}